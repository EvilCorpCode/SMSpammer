<resources>
    <!--
    my understanding on how this work is:
    name -> has to be the name of the vector drawable file (should be in drawable-v21
    so that the vector drawables are only used for lollipop and above)

    type -> if the device is pre lollipop we point to a different drawable to use
     -->
    <item name="ic_manage_24dp" type="drawable">@android:drawable/ic_menu_manage</item>
    <item name="ic_send_24dp" type="drawable">@android:drawable/ic_menu_send</item>
    <item name="ic_share_24dp" type="drawable">@android:drawable/ic_menu_share</item>
    <item name="ic_add_24dp" type="drawable">@android:drawable/ic_input_add</item>
    <item name="ic_save_24dp" type="drawable">@android:drawable/ic_menu_save</item>
    <item name="ic_about_24dp" type="drawable">@android:drawable/ic_menu_info_details</item>

    <item name="ic_thumb_up_24dp" type="drawable">@android:drawable/ic_media_ff</item>
</resources>
